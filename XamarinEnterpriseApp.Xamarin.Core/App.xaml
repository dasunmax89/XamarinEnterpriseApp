<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
        x:Class="XamarinEnterpriseApp.Xamarin.Core.App"
        xmlns:controls="clr-namespace:XamarinEnterpriseApp.Xamarin.Core.Controls;assembly=XamarinEnterpriseApp.Xamarin.Core"
        xmlns:converters="clr-namespace:XamarinEnterpriseApp.Xamarin.Core.Converters;assembly=XamarinEnterpriseApp.Xamarin.Core"
        xmlns:ios="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"
        ios:Application.EnableAccessibilityScalingForNamedFontSizes="True">
    <Application.Resources>
        <ResourceDictionary>
            <!-- CONVERTERS -->
            <converters:InverseBoolConverter x:Key="InverseBoolConverter" />
            <converters:MapIconConverter x:Key="MapIconConverter" />
            <converters:FormMapsPositionConverter x:Key="FormMapsPositionConverter" />
            <converters:GMapsPositionConverter x:Key="GMapsPositionConverter" />
            <converters:ImageSourceConverter x:Key="ImageSourceConverter" />
            <converters:StringNullOrEmptyBoolConverter x:Key="StringNullOrEmptyBoolConverter" />

            <!-- COLORS -->
            <Color x:Key="AppTitleBarColor">#63DF85</Color>
            <Color x:Key="AppTitleTextColor">#000000</Color>
            <Color x:Key="AppBackgroundColor">#ebe7e2</Color>
            <Color x:Key="AppFooterColor">#ffffff</Color>
            <Color x:Key="AppPrimaryTextColor">#000000</Color>
            <Color x:Key="EntryBackgroundColor">#F1F3F5</Color>
            <Color x:Key="EntryBorderColor">#d8d8d8</Color>
            <Color x:Key="EntryPlaceholderColor">#999999</Color>
            <Color x:Key="EntryTextColor">#2f5270</Color>
            <Color x:Key="ModalBackgroundColor">#CC000000</Color>
            <Color x:Key="ProgressIndicatorColor">#25557d</Color>
            <Color x:Key="ListItemSeparatorColor">#E7E7E7</Color>
            <Color x:Key="MenuSectionSeparatorColor">#9f9f9f</Color>
            <Color x:Key="HyperlinkColor">#4f79a2</Color>
            <Color x:Key="FormItemCaptionColor">#000000</Color>
            <Color x:Key="ActionSheetPopupColor">#000000</Color>
            <Color x:Key="CategoryListViewBGColor">#FAFAFA</Color>
            <Color x:Key="MenuBackgroundColor">#63DF85</Color>
            <Color x:Key="MenuSeparatorColor">#FAFAFA</Color>
            <Color x:Key="MenuTextColor">#000000</Color>

            <Color x:Key="WhiteColor">#ffffff</Color>
            <Color x:Key="BlackColor">#000000</Color>
            <Color x:Key="GrayColor">#e2e2e2</Color>
            <Color x:Key="AppLightGrayColor">#F1F3F5</Color>
            <Color x:Key="ErrorColor">#d00000</Color>
            <Color x:Key="AppBlueColor">#0775DB</Color>
            <Color x:Key="AppLightBlueColor">#9CC8F1</Color>
            <Color x:Key="AppOrangeColor">#FF9900</Color>
            <Color x:Key="AppGreenColor">#63DF85</Color>
            <Color x:Key="AppRedColor">#F11D00</Color>
            <Color x:Key="AppDarkGrayColor">#313143</Color>
            <Color x:Key="AppBlackColor">#000000</Color>
             <Color x:Key="AppBeigeColor">#ECE7E2</Color>


            <Color x:Key="AppGrayColor">#7E7E7E</Color>
            <Color x:Key="AppBlueGrayColor">#5B7185</Color>
            <Color x:Key="AppSummaryEditBlueColor">#0775DB</Color>

            
            <!--End of Colors-->
            <!--Font Sizes-->
            <!--End Font Sizes-->
            <!--Sizes-->
            <OnPlatform x:Key="TextEntryHeight" x:TypeArguments="x:Double">
                <On Platform="iOS" Value="48" />
                <On Platform="Android" Value="48" />
            </OnPlatform>
            <OnPlatform x:Key="DropdownHeight" x:TypeArguments="x:Double">
                <On Platform="iOS" Value="50" />
                <On Platform="Android" Value="50" />
            </OnPlatform>
            <OnPlatform x:Key="ButtonHeight" x:TypeArguments="x:Double">
                <On Platform="iOS" Value="48" />
                <On Platform="Android" Value="40" />
            </OnPlatform>
            <OnPlatform x:Key="ClearButtonHeight" x:TypeArguments="x:Double">
                <On Platform="iOS" Value="40" />
                <On Platform="Android" Value="40" />
            </OnPlatform>
            <OnPlatform x:Key="FooterHeight" x:TypeArguments="x:Double">
                <On Platform="iOS" Value="56" />
                <On Platform="Android" Value="56" />
            </OnPlatform>
            <OnPlatform x:Key="ListItemHeight" x:TypeArguments="x:Double">
                <On Platform="iOS" Value="60" />
                <On Platform="Android" Value="60" />
            </OnPlatform>
            <OnPlatform x:Key="TreeListItemHeight" x:TypeArguments="x:Double">
                <On Platform="iOS" Value="40" />
                <On Platform="Android" Value="40" />
            </OnPlatform>
            <OnPlatform x:Key="ActionListItemHeight" x:TypeArguments="x:Double">
                <On Platform="iOS" Value="50" />
                <On Platform="Android" Value="50" />
            </OnPlatform>

            <!--End of Sizes-->
            <!-- FONTS -->
            <OnPlatform x:Key="MediumFontSize" x:TypeArguments="NamedSize">
                <On Platform="iOS" Value="Medium" />
                <On Platform="Android" Value="Body" />
            </OnPlatform>
            <!--End of Fonts-->
            <!--Header Labels-->
            <!--Text-->
            <Style x:Key="CommonTextStyle" TargetType="{x:Type Label}">
                <Setter Property="FontFamily" Value="BasierCircle-Regular" />
            </Style>
            <Style x:Key="AppH1TextStyle" TargetType="{x:Type Label}"
                    BasedOn="{StaticResource CommonTextStyle}">
                <Setter Property="FontAttributes" Value="Bold" />
                <Setter Property="FontFamily" Value="BasierCircle-Bold" />

                <Setter Property="VerticalTextAlignment" Value="Center" />
                <Setter Property="FontSize" Value="Title" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="LineBreakMode" Value="WordWrap" />
                <Setter Property="TextColor"
                        Value="{StaticResource AppPrimaryTextColor}" />
            </Style>
            <Style x:Key="AppH1DarkTextStyle" TargetType="{x:Type Label}"
                    BasedOn="{StaticResource AppH1TextStyle}">
                <Setter Property="TextColor" Value="{StaticResource BlackColor}" />
            </Style>
            <Style x:Key="AppH1WhiteTextStyle" TargetType="{x:Type Label}"
                    BasedOn="{StaticResource AppH1TextStyle}">
                <Setter Property="TextColor" Value="{StaticResource WhiteColor}" />
            </Style>
            <Style x:Key="AppH2TextStyle" TargetType="{x:Type Label}"
                    BasedOn="{StaticResource CommonTextStyle}">
                <Setter Property="FontAttributes" Value="Bold" />
                <Setter Property="FontFamily" Value="BasierCircle-Bold" />

                <Setter Property="VerticalTextAlignment" Value="Center" />
                <Setter Property="FontSize" Value="Large" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="LineBreakMode" Value="WordWrap" />
                <Setter Property="TextColor"
                        Value="{StaticResource AppPrimaryTextColor}" />
            </Style>
            <Style x:Key="AppH2DarkTextStyle" TargetType="{x:Type Label}"
                    BasedOn="{StaticResource AppH2TextStyle}">
                <Setter Property="TextColor" Value="{StaticResource BlackColor}" />
            </Style>
            <Style x:Key="AppH3TextStyle" TargetType="{x:Type Label}"
                    BasedOn="{StaticResource CommonTextStyle}">
                <Setter Property="FontAttributes" Value="Bold" />
                <Setter Property="FontFamily" Value="BasierCircle-Regular" />

                <Setter Property="VerticalTextAlignment" Value="Center" />
                <Setter Property="FontSize" Value="Body" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="LineBreakMode" Value="WordWrap" />
                <Setter Property="TextColor"
                        Value="{StaticResource AppPrimaryTextColor}" />
            </Style>
            <Style x:Key="AppNormalTextStyle" TargetType="{x:Type Label}"
                    BasedOn="{StaticResource CommonTextStyle}">
                <Setter Property="FontAttributes" Value="None" />
                <Setter Property="FontFamily" Value="BasierCircle-Regular" />

                <Setter Property="VerticalTextAlignment" Value="Center" />
                <Setter Property="FontSize" Value="Body" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="LineBreakMode" Value="WordWrap" />
                <Setter Property="TextColor"
                        Value="{StaticResource AppPrimaryTextColor}" />
            </Style>
            <Style x:Key="AppParagraphTextStyle" TargetType="{x:Type Label}"
