            </Style>
            <Style x:Key="FormButtonWhiteStyle" TargetType="{x:Type Button}">
                <Setter Property="FontSize"
                        Value="{StaticResource MediumFontSize}" />
                <Setter Property="BackgroundColor"
                        Value="{StaticResource WhiteColor}" />
                <Setter Property="TextColor"
                        Value="{StaticResource AppBlueColor}" />
                <Setter Property="HeightRequest"
                        Value="{StaticResource ButtonHeight}" />
                <Setter Property="FontAttributes" Value="Bold" />
                <Setter Property="FontFamily" Value="BasierCircle-Bold" />
