                <Setter Property="BackgroundColor"
                        Value="{StaticResource AppBlueColor}" />
                <Setter Property="HeightRequest"
                        Value="{StaticResource ButtonHeight}" />
                <Setter Property="HasShadow" Value="False" />
                <Setter Property="CornerRadius" Value="10" />
                <Setter Property="Padding" Value="0,0" />
            </Style>
            <Style x:Key="FormImageButtonBlueDisabledStyle"
                    TargetType="{x:Type Frame}"
                    BasedOn="{StaticResource FormImageButtonBlueStyle}">
                <Setter Property="BackgroundColor"
                        Value="{StaticResource AppLightBlueColor}" />
