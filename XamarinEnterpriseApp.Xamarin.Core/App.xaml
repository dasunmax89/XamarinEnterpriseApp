                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="LineBreakMode" Value="WordWrap" />
                <Setter Property="TextColor"
                        Value="{StaticResource AppPrimaryTextColor}" />
                <Setter Property="FontFamily" Value="BasierCircle-Regular" />
            </Style>
            <Style x:Key="FormErrorMessageStyle" TargetType="{x:Type Label}">
                <Setter Property="FontSize" Value="Small" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="TextColor" Value="{StaticResource ErrorColor}" />
                <Setter Property="Margin" Value="0,0" />
                <Setter Property="Padding" Value="0,0" />
                <Setter Property="FontFamily" Value="BasierCircle-Regular" />
                <Setter Property="LineBreakMode" Value="WordWrap" />
            </Style>
            <Style x:Key="ClearButtonLabelStyle" TargetType="{x:Type Label}">
                <Setter Property="TextColor"
                        Value="{StaticResource AppBlueColor}" />
                <Setter Property="Margin" Value="5,5,5,5" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="FontAttributes" Value="None" />
                <Setter Property="FontSize"
                        Value="{StaticResource MediumFontSize}" />
                <Setter Property="FontFamily" Value="BasierCircle-Regular" />
            </Style>
            <!--Buttons-->
            <Style x:Key="LinkButtonLabelStyle" TargetType="{x:Type Label}">
                <Setter Property="VerticalTextAlignment" Value="Center" />
                <Setter Property="HorizontalTextAlignment" Value="Start" />
                <Setter Property="FontSize"
                        Value="{StaticResource MediumFontSize}" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="LineBreakMode" Value="WordWrap" />
                <Setter Property="TextColor"
                        Value="{StaticResource AppPrimaryTextColor}" />
                <Setter Property="FontFamily" Value="BasierCircle-Regular" />
            </Style>
            <Style x:Key="TransparentButtonLabelStyle"
                    TargetType="{x:Type Label}">
                <Setter Property="VerticalTextAlignment" Value="Center" />
                <Setter Property="HorizontalTextAlignment" Value="Center" />
                <Setter Property="FontSize"
                        Value="{StaticResource MediumFontSize}" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="LineBreakMode" Value="NoWrap" />
                <Setter Property="TextColor"
                        Value="{StaticResource HyperlinkColor}" />
                <Setter Property="FontAttributes" Value="Bold" />
                <Setter Property="FontFamily" Value="BasierCircle-Bold" />
            </Style>
            <Style x:Key="ClearButtonStyle" TargetType="{x:Type Button}">
               
                <Setter Property="Margin" Value="5,5,5,5" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="FontAttributes" Value="None" />
                <Setter Property="FontSize" Value="Medium" />
                <Setter Property="FontFamily" Value="BasierCircle-Regular" />
            </Style>
             <Style x:Key="ClearButtonBlackStyle" TargetType="{x:Type Button}" BasedOn="{StaticResource ClearButtonStyle}">
                <Setter Property="TextColor"
                        Value="{StaticResource AppDarkGrayColor}" />
                <Setter Property="BorderColor"
                        Value="{StaticResource AppDarkGrayColor}" />
                <Setter Property="BorderWidth" Value="1" />
                <Setter Property="CornerRadius" Value="8" />
                <Setter Property="Margin" Value="5,5,5,5" />
                <Setter Property="HeightRequest"
                        Value="48" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="FontAttributes" Value="None" />
                <Setter Property="FontSize" Value="Small" />
                <Setter Property="FontFamily" Value="BasierCircle-Bold" />
            </Style>
            <Style x:Key="FormButtonBlueStyle" TargetType="{x:Type Button}">
                <Setter Property="FontSize"
                        Value="{StaticResource MediumFontSize}" />
                <Setter Property="BackgroundColor"
                        Value="{StaticResource AppBlueColor}" />
                <Setter Property="TextColor" Value="{StaticResource WhiteColor}" />
                <Setter Property="HeightRequest"
                        Value="{StaticResource ButtonHeight}" />
                <Setter Property="FontAttributes" Value="Bold" />
                <Setter Property="CornerRadius" Value="10" />
                <Setter Property="FontFamily" Value="BasierCircle-Bold" />
            </Style>
            <Style x:Key="FormButtonBlueDisabledStyle"
