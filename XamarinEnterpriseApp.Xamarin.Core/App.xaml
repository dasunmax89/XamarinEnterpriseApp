                <Setter Property="FontFamily" Value="BasierCircle-Bold" />

            </Style>
            <Style x:Key="ListHeader2Style" TargetType="{x:Type Label}">
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="TextColor" Value="{StaticResource GrayColor}" />
                <Setter Property="Margin" Value="0,0" />
                <Setter Property="VerticalTextAlignment" Value="Center" />
                <Setter Property="HorizontalTextAlignment" Value="Start" />
                <Setter Property="FontSize" Value="Small" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="LineBreakMode" Value="WordWrap" />
                <Setter Property="FontAttributes" Value="None" />
                <Setter Property="FontFamily" Value="BasierCircle-Regular" />

            </Style>
            <Style x:Key="ListHeader3Style" TargetType="{x:Type Label}">
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="TextColor" Value="{StaticResource GrayColor}" />
                <Setter Property="Margin" Value="0,0" />
                <Setter Property="VerticalTextAlignment" Value="Center" />
                <Setter Property="HorizontalTextAlignment" Value="Start" />
                <Setter Property="FontSize" Value="Small" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="LineBreakMode" Value="WordWrap" />
                <Setter Property="FontAttributes" Value="None" />
                <Setter Property="FontFamily" Value="BasierCircle-Regular" />

            </Style>
            <!--End Header Labels-->
            <!--Form-->
            <Style x:Key="FormItemCaptionStyle" TargetType="{x:Type Label}">
                <Setter Property="FontSize" Value="Small" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="TextColor"
                        Value="{StaticResource FormItemCaptionColor}" />
                <Setter Property="Margin" Value="0,5,0,5" />
                <Setter Property="FontFamily" Value="BasierCircle-Regular" />
            </Style>
            <Style x:Key="FormItemCaptionStrongStyle"
                    TargetType="{x:Type Label}"
                    BasedOn="{StaticResource FormItemCaptionStyle}">
                <Setter Property="TextColor" Value="{StaticResource BlackColor}" />
                <Setter Property="FontAttributes" Value="Bold" />
                <Setter Property="FontFamily" Value="BasierCircle-Bold" />
            </Style>
            <Style x:Key="FormItemValueStyle" TargetType="{x:Type Label}">
                <Setter Property="Margin" Value="0,0" />
                <Setter Property="Padding" Value="0,0" />
                <Setter Property="TextColor" Value="{StaticResource BlackColor}" />
                <Setter Property="VerticalTextAlignment" Value="Center" />
                <Setter Property="HorizontalTextAlignment" Value="Start" />
                <Setter Property="FontSize"
                        Value="{StaticResource MediumFontSize}" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="LineBreakMode" Value="WordWrap" />
                <Setter Property="HorizontalOptions" Value="FillAndExpand" />
                <Setter Property="FontAttributes" Value="None" />
                <Setter Property="FontFamily" Value="BasierCircle-Regular" />
            </Style>
            <Style x:Key="DatePickerStyle" TargetType="{x:Type DatePicker}">
                <Setter Property="FontSize"
                        Value="{StaticResource MediumFontSize}" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="TextColor" Value="{StaticResource BlackColor}" />
